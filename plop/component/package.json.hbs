{
  "name": "@heroui-mobile/{{componentName}}",
  "version": "2.0.0",
  "description": "{{description}}",
  "keywords": [
    "{{componentName}}"
  ],
  "author": "jiezhiyong <vopjie@gmail.com>",
  "homepage": "https://github.com/jiezhiyong/heroui-mobile",
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiezhiyong/heroui-mobile.git",
    "directory": "packages/{{outDir}}/{{componentName}}"
  },
  "bugs": {
    "url": "https://github.com/jiezhiyong/heroui-mobile/issues"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "pnpm build:fast --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "react": ">=18 || >=19.0.0-rc.0",
