    "@heroui-mobile/system": ">=2.4.23"
  },
  "dependencies": {
    "@heroui-mobile/shared-utils": "workspace:*",
    "@heroui-mobile/react-utils": "workspace:*"
  },
  "devDependencies": {
    "@heroui-mobile/theme": "workspace:*",
    "@heroui-mobile/system": "workspace:*",
    "clean-package": "2.2.0",
    "react": "18.3.0",
    "react-dom": "18.3.0"
  },
